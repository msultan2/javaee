<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from MathOperations2Service WSDL service -->

  <service name="MathOperations2" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://Server"/>
      <parameter name="wsdlServiceElement" value="MathOperations2Service"/>
      <parameter name="schemaQualified" value="http://Server"/>
      <parameter name="wsdlServicePort" value="MathOperations2"/>
      <parameter name="className" value="Server.MathOperations2SoapBindingImpl"/>
      <parameter name="wsdlPortType" value="MathOperations2"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="addSum2" qname="operNS:addSum2" xmlns:operNS="http://Server" returnQName="retNS:addSum2Return" xmlns:retNS="http://Server" returnType="rtns:int" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:x" xmlns:pns="http://Server" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
        <parameter qname="pns:y" xmlns:pns="http://Server" type="tns:int" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="addSum2"/>

  </service>
</deployment>
